    "name": "Roy Miloh",
    "email": "roymi@wix.com"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./velo": {
      "import": "./dist/esm/velo-index.js",
      "require": "./dist/cjs/velo-index.js",
      "types": "./dist/types/velo-index.d.ts"
    },
    "./velo-events": {
      "import": "./dist/esm/velo-events.js",
      "require": "./dist/cjs/velo-events.js",
      "types": "./dist/types/velo-events.d.ts"
    },
    "./ambassador": {
      "import": "./dist/esm/ambassador-index.js",
      "require": "./dist/cjs/ambassador-index.js",
      "types": "./dist/types/ambassador-index.d.ts"
    },
    "./db-drivers": {
      "import": "./dist/esm/db-drivers.js",
      "require": "./dist/cjs/db-drivers.js",
      "types": "./dist/types/db-drivers.d.ts"
    },
    "./types": {
      "import": "./dist/esm/types.js",
      "require": "./dist/cjs/types.js",
      "types": "./dist/types/types.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "velo",
    "velo-events",
    "ambassador",
    "db-drivers",
    "src"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "npm run proto && yoshi-library build",
    "start": "yoshi-library start",
    "test": "yoshi-library test",
    "lint": "yoshi-library lint",
    "lint:fix": "npm run lint -- --fix",
    "posttest": "npm run lint",
    "proto": "wix-proto-codegen --module . --dir src/proto --filename index --stdCase --tsExportDefault --stringEnums --stringLongs",
    "example": "npm start --prefix example"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint"
  },
  "dependencies": {
    "@wix/metro-public-utils": "1.0.36",
    "@wix/motion-edm-autogen-p13n": "1.0.82",
    "@wix/motion-edm-autogen-transformations": "1.61.0",
    "@wix/motion-edm-autogen-transformations-core": "1.55.0",
    "js-base64": "^3.7.7",
    "lodash": "^4.17.21",
    "querystring": "^0.2.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.176",
    "@types/node": "^8.10.66",
    "@wix/com.wixpress.p13n.protos-common-proto": "^1.39.0",
    "@wix/http-client": "^2.0.0",
    "@wix/jest-yoshi-preset": "^6.95.1",
    "@wix/wix-proto-codegen": "^2.0.1156",
    "@wix/yoshi-flow-library": "^6.95.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "~4.2.5",
    "lint-staged": "^10.5.4",
    "ts-jest": "^29.1.1",
    "typescript": "~4.7.4"
  },
  "jest": {
    "preset": "@wix/jest-yoshi-preset"
  },
  "yoshiFlowLibrary": {
    "bundle": {
      "library": "counter"
    }
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress",
      "artifactId": "metro-runtime",
      "targets": {
        "static": true
      }
    }
  },
  "falconPackageHash": "c89bca43b8940dc1ebfe2f71d0d8a5d6742a58818b2c6cc9c37888f3"
